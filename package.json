{
  "name": "scoop",
  "version": "1.0.0",
  "scripts": {
    "fetch-env": "cd .. && cd frontend_v3 && direnv allow",
    "start": "NODE_ENV=production expo start --dev-client",
    "start-expo-go": "expo start --go",
    "start-expo-go-tunnel": "expo start --go --tunnel",
    "start-tunnel": "NODE_ENV=production expo start --dev-client --tunnel",
    "start-dev": "NODE_ENV=development expo start --dev-client",
    "start-dev-tunnel": "NODE_ENV=development expo start --dev-client --tunnel",
    "start-build": "NODE_ENV=development expo start",
    "start-build-tunnel": "NODE_ENV=development expo start --tunnel",
    "start-build-local": "NODE_ENV=local expo start",
    "start-build-local-linux": "set NODE_ENV=local && expo start --tunnel",
    "start-build-local-win": "set NODE_ENV=local && expo start",
    "clean-build": "NODE_ENV=development expo prebuild --clean",
    "clean-build-tunnel": "NODE_ENV=development expo prebuild --clean --tunnel",
    "clean-build-local": "NODE_ENV=local expo prebuild --clean",
    "clean-build-local-tunnel": "NODE_ENV=local expo prebuild --clean --tunnel",
    "clean-build-local-win": "set NODE_ENV=local && expo prebuild --clean",
    "android-local": "NODE_ENV=development expo run:android",
    "android": "NODE_ENV=development expo run:android",
    "android-tunnel": "NODE_ENV=development expo run:android --tunnel",
    "android-win": "set NODE_ENV=development && expo run:android",
    "ios-local": "NODE_ENV=local expo run:ios",
    "ios-local-tunnnel": "NODE_ENV=local expo run:ios --tunnel",
    "ios": "NODE_ENV=development expo run:ios",
    "ios-tunnel": "NODE_ENV=development expo run:ios --tunnel",
    "eas-ios-build": "NODE_ENV=production eas build --profile production --platform ios",
    "eas-ios-build-tunnel": "NODE_ENV=production eas build --profile production --platform ios --tunnel",
    "eas-ios-build-prod": "NODE_ENV=production eas build --profile production --platform ios",
    "eas-ios-build-prod-tunnel": "NODE_ENV=production eas build --profile production --platform ios --tunnel",
    "eas-update-prod": "NODE_ENV=production eas update",
    "eas-submit-ios": "eas submit -p ios --latest",
    "web": "expo start --web",
    "web-tunnel": "expo start --web --tunnel",
    "lint": "npx eslint src/ --fix --quiet",
    "format": "npx prettier --write ./src",
    "debugger": "open 'rndebugger://set-debugger-loc?host=localhost&port=19000'"
  },
