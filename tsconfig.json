{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true
  },
  "filesGlob": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "node_modules/typescript/lib/lib.es6.d.ts"
  ],
  "types": [
    "react",
    "react-dom",
    "react-native"
  ],
  "exclude": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.json",
  ]
}
